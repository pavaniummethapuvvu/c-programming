//write a c program to print palindrome or not
#include<stdio.h>
void main()
{
	int n,rev=0,r,t;
	printf("enter three digit number");
	scanf("%d",&n);
	t=n;
	while(n!=0)
	{
		r=n%10;
		rev=rev*10+r;
		n=n/10;
	}
	if(rev==t)
	{
	printf("palindrome");
    }
	else
	{
	printf("not a palindrome");
    }
}
