//write a c program to print the following pattern
  #include<stdio.h>
  void main()
  {
  	int i,j;
  	scanf("%d",&n);
	for(i=1;i<=5;i++)
  	{
  	for(j=1;j<=i;j++)
  	{
  		printf("%d\t",j);
  	}
      printf("\n");
    }
	for(i=4;i>0;i--)
	{
		for(j=1;j<=i;j++)
	   {
	   	printf("%d\t",j);
	   }
	   printf("\n");
   }
 }
