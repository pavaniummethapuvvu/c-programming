/write a c program to print revere of the given number
  #include<stdio.h>
  void main()
  {
  	int dummy,x,n,rev=0;
  	printf("enter a number");
  	scanf("%d",&n);
  	dummy=n;
  	while(n>0)
  	{
  		x=n%10;
  		rev=rev*10+x;
  		n=n/10;
	  }
	  printf("\nReversed number  of %d is %d",dummy,rev);
  }
